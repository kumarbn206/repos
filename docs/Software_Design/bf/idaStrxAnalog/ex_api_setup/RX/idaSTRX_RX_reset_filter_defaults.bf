RX1.IF_HB_ST2_CF.BINARY = 0X13;
RX1.IF_HB_ST2_CF.TURN_ON = 0XF;
RX1.IF_HB_STB.ST1 = 0X4A;
RX1.IF_HB_STB.ST2 = 0X4B;
RX1.IF_A.SW_VGA1_TO_VGA2 = 1;

// Get the controls back and the vga1-to-vga2 circuit open
RX1.IF_A.HB_RST_OVERWRITE = 0;
RX1.IF_A.HB_SW_VGA1_TO_VGA2 = 1;


// high bw
RX1.IF_HB_R.ST1_RF =  0X0;
RX1.IF_HB_R.ST2_RF =  0X0;
RX1.IF_HB_R.ST2_RLPF_BINARY = 0X0;
