
// Default values for teh filter while operating in reset mode
// HB  <- HFP=200KHz Gain_IF=34dB, LPF_IF=65MHz 


// ----   RX1 ------------------- 
// Profile for reset
RX1.IF_HB_ST1_CIN.UNARY = 0X3;
RX1.IF_HB_ST1_CIN.BINARY = 0X19;
RX1.IF_HB_ST1_CIN.TURN_ON = 0X3FF; 
RX1.IF_HB_ST1_CF.UNARY = 0X0;
RX1.IF_HB_ST1_CF.BINARY = 0X13;
RX1.IF_HB_ST1_CF.TURN_ON = 0XF
RX1.IF_HB_ST2_CIN.UNARY = 0X3;
RX1.IF_HB_ST2_CIN.BINARY = 0X19;
